{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "outDir": "../dist-main",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "types": ["node"],
    "declaration": false,
    "sourceMap": true
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules", "../src", "../dist", "../dist-main"]
}
